{
    "name": "nevadskiy/nova-collection-field",
    "description": "Collection fields for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "collection",
        "field",
        "morph-to-many"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nevadskiy\\Nova\\Collection\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nevadskiy\\Nova\\Collection\\FieldServiceProvider"
            ]
        }
    },
    "suggest": {
        "nevadskiy/laravel-many-to-morph": "Required to use ManyToMorphCollection field."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
